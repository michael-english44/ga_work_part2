   ],
   "source": [
    "# A\n",
    "sy.init_printing()\n",
    "def predictor(x): return lr.coef_*x + lr.intercept_\n",
    "print('The value of my prediction for {} is {}'.format(20, predictor(20)))\n",
    "print('The value of my prediction for {} is {}'.format(21, predictor(21)))\n"
   ]
  },
  {
